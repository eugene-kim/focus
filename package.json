{
  "name": "focus-app",
  "version": "0.1.0",
  "private": true,
  "config": {
    "server_path": "src/server/index.js",
    "knexfile_path": "src/server/database/knexfile.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.2.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-native": "^4.0.0",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "jest-plugin-set": "^2.6.0",
    "jest-plugins": "^2.6.0",
    "react-devtools": "^3.1.0",
    "react-test-renderer": "16.0.0",
    "regenerator-runtime": "^0.11.1"
  },
  "scripts": {
    "start": "react-native start -- --reset-cache",
    "node": "nodemon $npm_package_config_server_path --exec babel-node",
    "debug": "nodemon --inspect $npm_package_config_server_path --exec babel-node",
    "android": "react-native run-android",
    "ios": "react-native run-ios -- --reset-cache",
    "test": "jest --verbose",
    "test_debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "migrate": "knex migrate:latest --knexfile=$npm_package_config_knexfile_path",
    "create_migration": "knex migrate:make --knexfile=$npm_package_config_knexfile_path",
    "seed": "knex seed:run --knexfile=$npm_package_config_knexfile_path",
    "compile": "npx babel src --out-dir lib --source-maps",
    "react-devtools": "react-devtools"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./jest-plugins.js"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.7",
    "apollo-client": "^2.2.3",
    "apollo-link-http": "^1.3.3",
    "apollo-server-express": "^1.3.2",
    "body-parser": "^1.18.1",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.7.3",
    "graphql-tools": "^2.20.2",
    "immutable": "^3.8.2",
    "knex": "^0.13.0",
    "lodash": "^4.17.5",
    "morgan": "^1.9.0",
    "normalizr": "^3.2.4",
    "pg": "^7.4.1",
    "pluralize": "^7.0.0",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-native": "^0.53.0",
    "react-native-elements": "^0.18.5",
    "react-native-vector-icons": "^4.5.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "uuid": "^3.2.1"
  }
}
